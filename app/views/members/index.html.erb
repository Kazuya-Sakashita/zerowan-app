<h1>掲載中の里親募集</h1>

<div class="container">
  <%= render 'pets/pet_list', locals: { pets: @pets }%>
  <% if @show_more_link %>
    <div class="mt-3">
      <p><%= link_to 'もっと見る', members_pets_path(all: true, user_id: params[:user_id]), class: 'btn btn-secondary' %></p>
    </div>
  <% end %>
</div>
